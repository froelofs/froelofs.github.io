@incollection{Roelofsen:19OER,
	Abstract = {This survey article discusses two basic issues that semantic theories of questions face. The first is how to conceptualise and formally represent the semantic content of
questions. This issue arises in particular because the standard truth-conditional notion of
meaning, which has been fruitful in the analysis of declarative statements, is not applicable
to questions. The second issue is how questions, when embedded in a declarative statement
(e.g., in Bill wonders who called) contribute to the truth-conditional content of that statement. Several ways in which these issues have been addressed in the literature are discussed
and compared.},
	Author = {Roelofsen, Floris},
	Booktitle = {Oxford Research Encyclopedia of Linguistics},
	Date-Added = {2019-05-08 15:13:16 +0200},
	Date-Modified = {2019-05-08 15:15:34 +0200},
	Keywords = {inquisitive semantics,theoretical linguistics,questions,modality,attitude predicates,survey},
	Publisher = {Oxford University Press},
	Title = {Semantic theories of questions},
