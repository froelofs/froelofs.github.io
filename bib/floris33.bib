	Mendeley-Tags = {attitude predicates,inquisitive semantics,modality,questions,theoretical linguistics},
	Title = {{Picky predicates: Why believe doesn't like interrogative complements, and other puzzles}},
	Url = {https://link.springer.com/article/10.1007/s11050-019-09152-9},
